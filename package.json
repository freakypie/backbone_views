  "license": "BSD-2-Clause",
  "keywords": [
    "backbone",
    "views",
    "cbv"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/freakypie/backbone_views.git"
  },
  "devDependencies": {
    "chai": "^2.3.0",
    "chai-spies": "^0.6.0",
    "jsdom": "^8.0.2",
    "mocha": "^2.2.5",
    "jquery": "^2.2.0"
  },
  "peerDependencies": {
    "backbone": "^1.3.3"
  },
  "dependencies": {
    "store": "^1.3.20"
  }
}
