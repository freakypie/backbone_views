{
  "name": "backbone_views",
  "version": "0.7.0",
  "description": "A collection of views similar to django views",
  "main": "lib/index.coffee",
  "scripts": {
    "prepublish": "coffee --output lib --compile src",
    "compile": "coffee --output lib --compile src",
    "watch": "coffee --watch --output lib --compile src",
    "test_watch": "mocha --compilers coffee:coffee-script/register  --reporter list -w",
    "test": "mocha --compilers coffee:coffee-script/register --reporter nyan"
  },
